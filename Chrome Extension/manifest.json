{
  "name": "Text Postive",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "HACKMIT",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "browser_action":{
    "default_popup": "popup.html"
  },
  "permissions": [
      "<all_urls>",
      "storage"
  ],
  "content_scripts": [
    {
      "matches": [
          "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
