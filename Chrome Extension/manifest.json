{
  "name": "Text Postive",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "HACKMIT",
  "homepage_url": "http://extensionizr.com",
  "icons": {
    "16": "icons/textpositive_logo.png",
    "48": "icons/textpositive_logo.png",
    "128": "icons/textpositive_logo.png"
  },
  "default_locale": "en",
  "browser_action":{
    "default_popup": "popup.html"
  },
  "permissions": [
      "<all_urls>",
      "storage"
  ],
  "web_accessible_resources": [
	"icons/*"
  ],
  "content_scripts": [
    {
      "matches": [
          "<all_urls>"
      ],
      "js": [
        "src/inject/inject.js"
      ],
	  "css": [
	  	"style.css"
	  ]
    }
  ]
}
